    pitch = 0.8
    move = pos, -10to10, -10to10, -10to10
loop "generation/waterfall":
    volume = 0.05
    pitch = 1.0
    move = fly,0,10,0,1
loop "generation/crowd_of_people":
    volume = 0.2
    pitch = 1.0
    move = pos,0,0,0