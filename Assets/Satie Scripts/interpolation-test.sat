# Test script for interpolation functionality

# Using goto - goes from 0 to 0.5 and stays at 0.5
group cars:
    loop "generation/car_loop":
        volume = goto(0and0.01 as incubic in 4)
        pitch = gobetween(0.5and1.2 as incubic in 15)
        move = walk,-20to10,-15to15,1
        visual = trail

    loop "music/drone":
        volume = 0.3
        pitch = 0.4to0.9
        move = walk,-10to0, -5to5,1
        visual = sphere